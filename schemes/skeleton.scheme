[scheme]
processor = cage-asset-processor skeleton
index = 13

[bake_model]
display = bake model
type = bool
default = false

[axes]
display = axes
type = string
default = +x+y+z

[scale]
display = scale
type = real
default = 1

[bone_names]
display = export bone names
type = bool
default = true

[animation_names]
display = animation dependencies based on this asset's name
type = bool
default = true
